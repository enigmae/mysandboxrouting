{
  "compilerOptions": {
    "noImplicitAny": false,
    "module": "commonjs",
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es6",
    "skipDefaultLibCheck": true,
    "moduleResolution": "node",
    "jsx": "react",
    "strictNullChecks": false,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "preserveSymlinks": true
  },
  "exclude": [
    "node_modules"
  ]
}
